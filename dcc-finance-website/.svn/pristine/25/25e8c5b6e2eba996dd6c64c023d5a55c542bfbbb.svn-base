}
/*====================================*/
.footer {
  line-height: 150px
}

.footer a.logo:hover{
  text-decoration: none;
}
.footer .logo img {
  height:130px;
}
footer .logo .footer-tt{
  font-size:18px;
  color:#6b61e1;
  font-family: Arial, Helvetica, sans-serif;
  margin-left:-15px;
}
.footer .shares a img{
  max-width: 100%;
  margin-right: 1px;
}

.descList {
  display: flex;
}

.asideNav {
  position: fixed;
  padding-top: 50px;
  top: 100px;
  z-index: 99;
  right: 20px;
}

.asideNav ul {
  line-height: 30px;
  list-style: none;
  padding: 30px;
  background: #fff;
  border-radius: 4px;
  box-shadow: 0px 0px 10px #ddd;
}

.asideNav .asideNavBtn {
  width: 50px;
  height: 50px;
  position: absolute;
  top: 0;
  right: 0;
}

.asideNav .asideNavBtn.open {
  background: url(../images/asideNav/open.png);
}

.asideNav .asideNavBtn.close {
  background: url(../images/asideNav/close.png);
}

.asideNav a {
  color: #000;
}

.asideNav a.active {
  color: #6b61e1;
}

.asideNav a.backToTop {
  display: block;
  margin-top: 50px;
}
@media (max-width:1800px){
  .person,.investor .investorName{padding:0 175px;}
}
@media (max-width:1200px) {
  .visionAndMission {
    margin-top: 50px;
  }
  .person,.investor .investorName{padding:0 80px;}
  #banner {
    padding-bottom: 50px;
  }
  .queryInvest {
    margin-top: 68px
  }

}

@media (max-width:992px) {
 
  /* .visionAndMission {
