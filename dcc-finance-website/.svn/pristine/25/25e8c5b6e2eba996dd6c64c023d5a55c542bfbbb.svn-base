  }
  .countDownWidget_left, .countDownWidget_body, .countDownWidget_body+div {
    text-align: center;
  }
  .footer div{
    text-align: center;
  }

}

@media (max-width:767px) {
  .saleAds{
    font-size:14px;
  }
  #banner{
    padding-top:200px;
  }
  .banner .saling p{
    width:380px;
    font-size:18px;
    line-height:28px;
  }
   .banner .saling .icoImg {
    height: 24px;
  }
  .banner .saleTotal{
    width:300px;
    float:none;
    margin:0 auto;
  }
  .banner .getDcc{
