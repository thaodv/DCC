.nofloat{
  float:none!important;
}
.bg-gray {
  color: #f0f0f0;
}

.navbar-fixed-magin {
  margin-bottom: -150px;
  background: transparent;
  border-color: transparent;
  z-index: 9;
  border: none;
}

input::-webkit-input-placeholder{
 /* WebKit browsers */
color: #ccc;
}
input:-moz-placeholder{
/* Mozilla Firefox 4 to 18 */
color: #ccc;
}
input::-moz-placeholder{
 /* Mozilla Firefox 19+ */
color: #ccc;
}
input:-ms-input-placeholder{
 /* Internet Explorer 10+ */
color: #ccc;
}
#beforeSalTip{
  display:none;
}
#saleTip{
  font-size:17px;
  padding:10px 0;
  float:none;
  margin:0 auto;
  text-align:left;
}
#saleTip em,#saleTip span{
  font-size:21px;
}

/*===============================*/
.section-header {
  margin-top: 50px;
  font-size: 30px;
  text-align: center;
}

.section-body {
  padding: 50px 0;
}

.section-body>p {
  color: #7d788d;
  line-height: 40px;
  font-size: 18px;
}

.section-body h4 {
  line-height: 40px;
  font-size: 20px;
}

/*=============================*/
.navbar-inverse {
  background: transparent;
}

#banner {
  padding-top: 250px;
  padding-bottom: 150px;
  background: url('../images/bannerBg.png') center center;
  color: #fff;
  background-size:100% 100%;
}

h1 {
  font-size: 72px;
}

.countDownWidget {
  margin-top: 60px;
}

.countDownWidget_left {
  text-align: right;
  line-height: 60px;
}

.countDownWidget_body {
  margin-bottom: 20px;
  text-align: left;
}

.countDownWidget_body+div {
  text-align: left;
}

.countDownWidget_body_colon {
  width: 15px;
  font-weight: bold;
  line-height: 60px;
  vertical-align: top;
}

.countDownWidget_body_unit, .countDownWidget_body_colon {
  text-align: center;
  display: inline-block;
}

.countDownWidget_body_unit big {
  font-size:37px;
  line-height: 60px;
  display: block;
  font-weight: bold;
  border: 1px solid #fff;
  border-radius: 4px;
  width: 60px;
  height: 60px;
  margin-bottom: 5px;
}

.countDownWidget_body_unit small {
  display: block;
  line-height: 1;
}
/* saling progress */
#afterSale{
  display:none;
}
#afterSale .qrCode,#beforeSalTip .qrCode{
  width:20px;
  height:20px;
  position: relative;
  top:5px;
  display:inline-block;
  background:url("../images/erweima.png") no-repeat center center;
  background-size:100% 100%;
}
#afterSale .qrCode img,#beforeSalTip .qrCode img{
  position: absolute;
  top:25px;
  left:50%;
  transform: translate(-50%);
  width:100px;
  height:100px;
  z-index:1000;
  display:none;
}
#afterSale .qrCode:hover img,#beforeSalTip .qrCode:hover img{
  display:block;
}
.banner .saling p{
  width: 630px; 
  /* padding: 0 40px; */
  /* line-height: 80px; */
  font-size: 27px;
  padding: 30px;
  display: inline-block;
  background: rgba(0,0,0,0.2);
  margin-top: 60px;
}
.banner .saling span{
  font-size:14px;
}
.banner .saling .tk{
  vertical-align: middle;
}
.banner .saling .icoImg{
  height:30px;
  margin-left:5px;
}
.saleAds{
  float: none;
  margin: 0 auto 20px;
  font-size: 17px;
  /* white-space: nowrap; */
  word-wrap: break-word;
  text-align: left;
  white-space: normal;
  padding:0;
}
#beforeSalTip>p{
  margin:0 auto;
}
.saleProgress{
  min-height:60px;
}
.banner .saleProgress .progress{
  margin:40px 0 0 0;
  height:12px;
  border-radius: 10px;
  background:#dde3e8;
}
.banner .saleProgress .progress .progress-bar{
  background:linear-gradient(to right, #28c1fd, #695ce0);
  background:-webkit-linear-gradient(to right, #28c1fd, #695ce0);
  border-radius: 5px;
  text-align:right;
}
.banner .saleTotal{
  width:500px;
  padding:15px 20px;
  /* height:70px; */
  text-align:left;
  font-size:14px;
  float:left;
}
.banner .saleTotal>p{
  font-size:16px;
}
.banner .proMark{
  position: absolute;
  padding:0 10px;
  background:#6860dc;
  top:-30px;
  border-radius:5px;
 transform: translate(-43%);
 white-space: nowrap;
}
.banner .proMark:after{
  position: absolute;
  width: 0;
  height: 0;
  content: " ";
  top: 20px;
  left: 50%;
  margin-left: -10px;
  border-top: 5px solid #6860dc;
  border-left: 5px solid transparent;
  border-right: 5px solid transparent;
}
.banner .getDcc{
  width:94px;
  height:37px;
  font-size:16px;
  line-height:37px;
  padding:0 10px;
  border-radius: 5px;
  box-sizing: content-box;
  margin:77px 0 0 10px;
  float:left;
  color:#fff;
  text-decoration:none;
  background:linear-gradient(to bottom, #28c1fd, #695ce0);
  background:-webkit-linear-gradient(to bottom, #28c1fd, #695ce0);
}
.kycBg{
  width:300px;
  float:none!important;
  height:50px;
  color:#fff;
  background:linear-gradient(to bottom, #28c1fd, #695ce0);
  background:-webkit-linear-gradient(to bottom, #28c1fd, #695ce0);
  
}
.kycBg.btn{
  border:none;
}
.btn.focus, .btn:focus, .btn:hover{
  color:#fff!important;
}

#queryBtn {
  background: url('../images/subscribeBtn.png') center center;
  cursor: pointer;
  width: 107px;
  height: 39px;
  display: inline-block;
  color: #fff;
  text-align: center;
  line-height: 39px;
  font-size: 16px;
  border-radius: 10px;
}

.queryInvest {
  margin-top: 75px;
  text-align: right;
}

.queryInvest input {
  width: 290px;
  height: 42px;
  line-height: 42px;
  font-size: 16px;
  border: 1px solid #fff;
  background: transparent;
  border-radius: 6px;
  padding: 10px;
  display: inline-block;
}

/*=====================================*/
.visionAndMissionAndProblems {
  position: relative;
  z-index: 7;
  background: #fff;
  color: #666;
}

.visionAndMission {
  margin-top: -100px;
  padding: 1px 100px;
  background: #fff;
  border-radius: 4px;
  box-shadow: 0px 0px 30px #ddd;
}

.problems .circle {
  margin-right: 20px;
  display: inline-block;
  width: 82px;
  height: 82px;
  line-height: 82px;
  font-size: 30px;
  color: #6b5ce4;
  text-align: center;
  box-shadow: 0px 0px 30px #ddd;
  border-radius: 50%;
}

.problems h4 {
  display: inline-block;
}

.problems p {
  padding: 0 100px 50px;
}

.problems .col-lg-4>div:last-child {
  padding-left: 107px;
}

/*====================================*/
.howWeSolveTheProblems {
  background: #f0f0f0;
}

.howWeSolveTheProblems .card {
  max-width: 100%;
  margin: 60px auto;
  position: relative;
  background: #fff;
  width: 365px;
  height: 250px;
  border-radius: 8px;
  padding: 20px 35px;
}

.howWeSolveTheProblems .card img {
  position: absolute;
  left: 50%;
  top: 0;
  margin-left: -45px;
  margin-top: -91px;
}

.howWeSolveTheProblems .card p {
  color: #7d788d;
}

.howWeSolveTheProblems .card h4 {
  text-align: center;
  margin-bottom: 20px;
}

/*====================================*/
.senerios .card {
  width: 256px;
  height: 216px;
  margin: 50px auto 0;
  position: relative;
}

.senerios .card p {
  position: absolute;
  top: 135px;
  color: #fff;
  width: 100%;
  text-align: center;
}

.senerios .section-body>p {
  padding: 0 100px;
}

/*====================================*/
.mapList {
  background: #fff;
}

.mapShowList:last-child .line {
  display: none;
}

/*====================================*/
.strategicPartners,.exchanges {
  text-align: center;
  background: #f0f0f0;
}

.strategicPartners a,.exchanges a {
  display: inline-block;
  margin: 20px 15px;
}
.strategicPartners a img{
  height:111px;
}

/*====================================*/
.CoreTeam .personCard {
  width: 300px;
  height: 380px;
  box-shadow: 0px 0px 30px #ddd;
}

.CoreTeam .personCard .detail {
  height: 175px;
}

/*====================================*/
.investor {
  background: url(../images/investor/bg.png) center center;  background-size: 100% 100%
}

.investor .section-header {
  color: #fff;
}

.investor .section-body>p {
  color: #fff;
  padding: 0 100px;
}

.investor h4 {
  color: #542e8e;
}
/* 
.investor .personCard {
  height: 500px;
} */

.personCard h4 small {
  color: #542e8e !important;
}

/*====================================*/

.personCard {
  position: relative;
  width: 315px;
  max-width: 100%;
  padding: 60px 30px 30px 30px;
  margin: 105px auto 0;
  border-radius: 4px;
  background: #fff;
}

.personCard .header {
  position: absolute;
  width: 98px;
  height: 98px;
  top: -49px;
  left: 50%;
  margin-left: -49px;
}

.personCard .header .linkin {
  position: absolute;
  bottom: 5px;
  right: 5px;
}

.personCard .header>img {
  /* width: 100%; */
  width:90px;
  height: 90px;
  border-radius: 70px;
}

.personCard h4 {
  text-align: center;
  margin-bottom: 30px;
}

.personCard h4 small {
  display: block;
}

.personCard p {
}

.personCard .des {
  margin-left: -30px;
  line-height: 24px;
}

.personCard .detail {
  height: 300px;
  overflow: auto;
  line-height: 24px;
}

.personCard .personCardBtn {
  width: 32px;
  height: 32px;
  position: absolute;
  right: 30px;
  bottom: 30px;
}

.personCard .personCardBtn.add {
  background: url(../images/coreTeam/add.png);
}

.personCard .personCardBtn.delete {
  background: url(../images/coreTeam/delete.png);
}
/* 修改personCard css */
.person{
  padding:0 300px;
}
.person .personRight h4{
  width:150px;
}
.person .personCon{
  height:100%;
  display: flex;
  padding-left:77px;
  margin-bottom:20px;
 justify-content: center;
 align-items: center;
}
.person .personLeft{
  height:98px;
}
.person .personLeft img{
  display:inline-block;
  height:98px;
  margin-right:20px;
}
.person .personRight{
  line-height:20px;
  color:#fff;
  font-style: normal;
}
.person .personRight h4{
