<script>
    $(function () {
        $(".personCardBtn").on("click",function(){
            $(this).toggleClass("add delete")
            $(this).siblings(".detail, .des").toggleClass("hidden")
        })
        $(".asideNavBtn").on("click",function(){
            $(this).toggleClass("open close")
            $(this).siblings("ul").toggleClass("hidden")
        })
        $(".asideNav>ul a:not(.backToTop)").on("click",function(){
            $(this).addClass("active").end()
            $(this).parent().siblings().find('a').removeClass('active')
        })

        
    })
</script>
