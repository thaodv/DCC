.investPopover {
  display: none;
  position: absolute;
  z-index: 5;
  top: 76px;
  left: -80px;
  width: 1000px;
  color: #000;
  background: transparent;
  border:1px solid rgb(100,97,235);
  border-radius: 10px
}
 .investPopover:before{
  position: absolute;
  width: 0;
  height: 0;
  left: 50%;
  margin-left: -10px;
  margin-top:-19px;
  content: " ";
  border-bottom:20px solid rgb(100,97,235);
  border-left: 15px solid transparent;
  border-right: 15px solid transparent;
}
.investPopover:after{
  position: absolute;
  width: 0;
  height: 0;
  content: " ";
  top: -18px;
  left: 50%;
  margin-left: -10px;
  border-bottom: 20px solid #f0f0f0;
  border-left: 15px solid transparent;
  border-right: 15px solid transparent;
}
.investPopover .top h3 {
  width: 100%;
  height: 36px;
  line-height: 36px;
  margin: 16px 0 0;
  font-size: 16px;
  text-align: center
}

.tokenInfo input {
  display: inline-block;
  float: left;
  margin-right: 10px;
  color:rgb(161,165,169);
  text-indent: .5em;
  background-color: transparent;
  border: 1px solid rgb(100,97,235);
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
  border-radius: 10px
}
.tokenInfo input:focus{outline:none;}
.investPopover .middle b,.investPopover .middle span{
  background:rgb(100,97,235);
  border:1px solid rgb(100,97,235);
}

.investPopover .middle{width:85%;height:12px}
.investPopover .middle b,.investPopover .middle span{
  display:inline-block;float:left}
  .investProgress h3{
    width:100%;height:36px;line-height:36px;font-size:18px;color:#FFF;margin:20px 0 0
  }
    .investProgress h2{width:100%;height:50px;line-height:50px;font-size:24px;color:#FFF;margin:0}
    .investProgress h2 small{font-size:16px;color:#FFF}
