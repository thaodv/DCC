body {
  font-family: Helvetica Neue, Helvetica, Arial, sans-serif, Microsoft YaHei, STHeitiSC-Light, simsun, WenQuanYi Zen Hei, WenQuanYi Micro Hei, sans-serif;
  margin: 0;
  padding: 0;
  -webkit-font-smoothing: antialiased
}

body .container-fluid {
  width: 1260px
}

.contactIcon {
  overflow: hidden;
  background-image: url(../images/contactIcon.png);
  background-repeat: no-repeat
}

.contactIcon-email {
  float: left;
  width: 26px;
  height: 26px;
  background-position: 0 0
}

.contactIcon-Telegram {
  float: left;
  width: 25px;
  height: 25px;
  background-position: -1px -36px
}

