.about-view,
.about-view .about-int,
.contactIcon,
.scenarios-view {
  background-repeat: no-repeat;
}

.about-view h2,
.innovate-view h2,
.parnter-view a,
body .container-fluid h2 {
  text-align: center
}

body {
  font-family: Helvetica Neue, Helvetica, Arial, sans-serif, Microsoft YaHei, STHeitiSC-Light, simsun, WenQuanYi Zen Hei, WenQuanYi Micro Hei, sans-serif;
  margin: 0;
  padding: 0;
  -webkit-font-smoothing: antialiased
}

body .container-fluid {
  width: 1260px
}

body .container-fluid p {
  font-size: 12px
}

body .container-fluid h2 {
  font-size: 30px;
  margin: 64px 0
}

.contactIcon {
  overflow: hidden;
  background-image: url(../images/contactIcon.png)
}

.contactIcon-email {
  float: left;
  width: 26px;
  height: 26px;
  background-position: 0 0
}

.contactIcon-Telegram {
  float: left;
  width: 25px;
  height: 25px;
  background-position: -1px -36px
}

