                <tbody id="con" >
                    <script id="js-tmp" type="text/html">
                      {{if resultList.length>0}}
                        {{each resultList  value i}}
                        <tr>
                            <td title={{value.tip}}><div class="userAdd"><span>{{i+1}} </span>{{value.address}}</div></td>
                            <td>{{value.score}}</td>
                            <td>{{value.amount}}</td>
                          </tr>
                        {{/each}}
                      {{else}}
                      <tr><td colspan="3"> 暂无排行...</td></tr>
                      {{/if}}
                  </script>
                </tbody>
