  <section class="reward-view reward-ranking" >
      <div class="container-fluid">
        <div class="row">
          <div class="panel panel-default" style="padding-bottom:40px;">
              <div class="panel-body">
                  <div class="reward-title">
                      &nbsp;&nbsp;&nbsp;&nbsp;<span class="panel-title explorer-panel-title">Yesterday's Reward List</span>
                  </div>
              </div>
              <table>
                <thead>
                  <tr>
                    <th style="width:500px">User Address</th>
                    <th>Eco Values</th>
                    <th>DCC Rewarded</th>
                  </tr>
                </thead>
                <tbody id="con" >
                    <script id="js-tmp" type="text/html">
                      {{if resultList.length>0}}
                        {{each resultList  value i}}
                        <tr>
                          <td title={{value.tip}}><div class="userAdd"><span>{{i+1}} </span>{{value.address}}</div></td>
                          <td>{{value.score}}</td>
                          <td>{{value.amount}}</td>
                        </tr>
                        {{/each}}
                        {{else}}
                        <tr><td colspan="3"> 暂无排行...</td></tr>
                        {{/if}}
                  </script>
                </tbody>
              </table>
          </div>
      </div>
