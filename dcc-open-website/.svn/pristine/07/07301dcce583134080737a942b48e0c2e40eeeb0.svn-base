body {
  font-family: Helvetica Neue, Helvetica, Arial, sans-serif, Microsoft YaHei, STHeitiSC-Light, simsun, WenQuanYi Zen Hei, WenQuanYi Micro Hei, sans-serif;
  margin: 0;
  padding: 0;
  -webkit-font-smoothing: antialiased
}

body .container-fluid {
  width: 1260px
}

body .container-fluid p {
  font-size: 12px
}

body .container-fluid h2 {
  font-size: 26px;
  text-align: center;
  margin: 64px 0
}

.contactIcon {
  overflow: hidden;
  background-image: url(../images/contactIcon.png);
  background-repeat: no-repeat
}

.contactIcon-email {
  float: left;
  width: 26px;
  height: 26px;
  background-position: 0 0
}

.contactIcon-Telegram {
  float: left;
  width: 25px;
  height: 25px;
  background-position: -1px -36px
}



section[class$='-view'] {
  display: flex
}

section[class$='-view'].center-view {
  justify-content: center;
  align-items: center
}

.thumb-view {
  flex-direction: column;
  height: 100vh;
  min-height: 700px
}

.thumb-view img {
  margin: 160px 0 40px
}

.thumb-view h2 {
  color: #6b61e1;
  margin: 0
}

.thumb-view p {
  color: #6b61e1;
  font-size: 20px
}

